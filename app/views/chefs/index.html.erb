<h1>Listing chefs</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Username</th>
    <th>Rating</th>
    <th>Location</th>
    <th>Bio</th>
    <th>Specialty</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @chefs.each do |chef| %>
  <tr>
    <td><%= chef.id %></td>
    <td><%= chef.name %></td>
    <td><%= chef.username %></td>
    <td><%= chef.rating %></td>
    <td><%= chef.location %></td>
    <td><%= chef.bio %></td>
    <td><%= chef.specialty %></td>
    <td><%= link_to 'Show', chef %></td>
    <td><%= link_to 'Edit', edit_chef_path(chef) %></td>
    <td><%= link_to 'Destroy', chef, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Chef', new_chef_path %>
